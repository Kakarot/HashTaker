<?xml version="1.0" encoding="utf-8" ?>
<HashTaker>
  
<!--Add new extension elements containing the extensions you wish to HASH-->
  <Extensions>
    <extension>.exe</extension>
    <extension>.dll</extension>
    <extension>.dmg</extension>
</Extensions>

  <!--Directories to be parsed relative to the current directory-->
  <Locations IncludeCurrentDirectory ="yes">
    <!--<location>AnotherFolder</location>-->
    <!--<location>AnotherFolder2</location>-->
  </Locations>

	<!--Only one format can be specified at a time-->
  <Format>csv</Format>

	<!--Only one algorithm can be specified at a time-->
	<Algorithm>SHA1</Algorithm>
  
</HashTaker>    